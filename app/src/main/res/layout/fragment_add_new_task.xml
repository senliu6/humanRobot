<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="20dp"
    android:background="@mipmap/login_dabeijing4_21">

    <LinearLayout
        android:id="@+id/return_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="13dp"
            android:layout_height="21dp"
            android:layout_marginLeft="120dp"
            android:scaleType="fitXY"
            android:src="@mipmap/choosetask_jiantouzuo4_21" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dp"
            android:text="新建任务"
            android:textColor="#ff02fde8"
            android:textSize="22sp" />
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/add_new_task_text_margin_left"
        android:layout_marginBottom="@dimen/add_new_task_text_margin_bottom"
        android:text="重复"
        android:textColor="#ff02a496"
        android:textSize="21sp"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintStart_toStartOf="@+id/textView11" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/add_new_task_text_margin_left"
        android:layout_marginBottom="@dimen/add_new_task_text_margin_bottom"
        android:text="时间"
        android:textColor="#ff02a496"
        android:textSize="21sp"
        app:layout_constraintBottom_toTopOf="@+id/textView12"
        app:layout_constraintStart_toStartOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="@dimen/add_new_task_width"
        android:layout_height="@dimen/add_new_task_height"
        android:background="@drawable/tasks_report_text_shape"
        android:gravity="center_vertical"
        android:text="空跑"
        android:textColor="#ff02fde8"
        android:textSize="@dimen/add_new_task_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/textView12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="@dimen/add_new_task_width"
        android:layout_height="@dimen/add_new_task_height"
        android:background="@drawable/tasks_report_text_shape"
        android:gravity="center_vertical"
        android:text="空跑"
        android:textColor="#ff02fde8"
        android:textSize="@dimen/add_new_task_text_size"
        app:layout_constraintBottom_toTopOf="@+id/textView15"
        app:layout_constraintEnd_toStartOf="@+id/textView11"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/return_ll" />


    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/add_new_task_text_margin_left"
        android:layout_marginBottom="@dimen/add_new_task_text_margin_bottom"
        android:text="地图"
        android:textColor="#ff02a496"
        android:textSize="21sp"
        app:layout_constraintBottom_toTopOf="@+id/textView15"
        app:layout_constraintStart_toStartOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="@dimen/add_new_task_text_margin_bottom"
        android:text="任务"
        android:textColor="#ff02a496"
        android:textSize="21sp"
        app:layout_constraintBottom_toTopOf="@+id/textView19"
        app:layout_constraintStart_toStartOf="@+id/textView19" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="@dimen/add_new_task_width"
        android:layout_height="@dimen/add_new_task_height"
        android:background="@drawable/tasks_report_text_shape"
        android:gravity="center_vertical"
        android:text="空跑"
        android:textColor="#ff02fde8"
        android:textSize="@dimen/add_new_task_text_size"
        app:layout_constraintBottom_toTopOf="@+id/textView19"
        app:layout_constraintEnd_toEndOf="@+id/textView12"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="@dimen/add_new_task_width"
        android:layout_height="@dimen/add_new_task_height"
        android:background="@drawable/tasks_report_text_shape"
        android:gravity="center_vertical"
        android:text="空跑"
        android:textColor="#ff02fde8"
        android:textSize="@dimen/add_new_task_text_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView15"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/add_new_task_text_margin_left"
        android:layout_marginBottom="@dimen/add_new_task_text_margin_bottom"
        android:text="循环"
        android:textColor="#ff02a496"
        android:textSize="21sp"
        app:layout_constraintBottom_toTopOf="@+id/textView16"
        app:layout_constraintStart_toStartOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="@dimen/add_new_task_width"
        android:layout_height="@dimen/add_new_task_height"
        android:background="@drawable/tasks_report_text_shape"
        android:gravity="center_vertical"
        android:text="空跑"
        android:textColor="#ff02fde8"
        android:textSize="@dimen/add_new_task_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/textView15"
        app:layout_constraintEnd_toEndOf="@+id/textView11"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/textView11" />

    <TextView
        android:id="@+id/button"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:background="@drawable/button_background_selector"
        android:gravity="center"
        android:text="取消"
        android:textColor="#F4F1F1"
        android:textSize="@dimen/add_new_task_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/textView19"
        app:layout_constraintEnd_toStartOf="@+id/button2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/textView16" />

    <TextView
        android:id="@+id/button2"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:background="@drawable/button_background_selector"
        android:gravity="center"
        android:text="确认"
        android:textColor="#F4F1F1"
        android:textSize="@dimen/add_new_task_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/button"
        app:layout_constraintEnd_toEndOf="@+id/textView16"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button"
        app:layout_constraintTop_toTopOf="@+id/button" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="30dp"
        android:layout_height="25dp"
        android:layout_marginEnd="24dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/textView15"
        app:layout_constraintEnd_toEndOf="@+id/textView15"
        app:layout_constraintTop_toTopOf="@+id/textView15"
        app:srcCompat="@mipmap/login_jiantou4_21" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="30dp"
        android:layout_height="25dp"
        android:layout_marginEnd="24dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="@+id/textView11"
        app:layout_constraintTop_toTopOf="@+id/textView11"
        app:srcCompat="@mipmap/login_jiantou4_21" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="30dp"
        android:layout_height="25dp"
        android:layout_marginEnd="24dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/textView19"
        app:layout_constraintEnd_toEndOf="@+id/textView19"
        app:layout_constraintTop_toTopOf="@+id/textView19"
        app:srcCompat="@mipmap/login_jiantou4_21" />


</androidx.constraintlayout.widget.ConstraintLayout>