<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:layout_marginStart="130dp"
    android:layout_marginTop="114dp"
    android:layout_marginEnd="130dp"
    android:layout_marginBottom="63dp"
    android:background="@mipmap/health_8beijing4_21"
    android:orientation="vertical"
    android:scaleType="fitXY">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="124dp"
        android:layout_marginTop="76dp"
        android:text="任务进度"
        android:textColor="#ff02fde8"
        android:textSize="23sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textColor="#ff02fde8"
        android:textSize="128sp"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="execute_task_1"
        android:textColor="#ff02fde8"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="作业中_1 "
        android:textColor="#ff02fde8"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="%"
        android:textColor="#ff02fde8"
        android:textSize="40sp"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        tools:layout_editor_absoluteY="189dp" />

    <ImageView
        android:id="@+id/imageCleanV"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="128dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/performtask_zanting4_21" />

    <ImageView
        android:id="@+id/imageCleanBt"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginTop="20dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="@+id/imageCleanV"
        app:layout_constraintStart_toStartOf="@+id/imageCleanV"
        app:layout_constraintTop_toBottomOf="@+id/imageCleanV"
        app:srcCompat="@mipmap/performtask_beijingkuang4_21" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:shadowColor="#8f0a3658"
        android:shadowDx="0"
        android:shadowDy="1"
        android:shadowRadius="3.0"
        android:text="清洁设置"
        android:textColor="#fff8fafd"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageCleanBt"
        app:layout_constraintEnd_toEndOf="@+id/imageCleanBt"
        app:layout_constraintStart_toStartOf="@+id/imageCleanBt"
        app:layout_constraintTop_toTopOf="@+id/imageCleanBt" />


    <ImageView
        android:id="@+id/imageLocationV"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:layout_marginStart="30dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toEndOf="@+id/imageCleanV"
        app:layout_constraintTop_toTopOf="@+id/imageCleanV"
        app:srcCompat="@mipmap/performtask_zanting4_21" />

    <ImageView
        android:id="@+id/imageLocationBt"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginTop="20dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="@+id/imageLocationV"
        app:layout_constraintStart_toStartOf="@+id/imageLocationV"
        app:layout_constraintTop_toBottomOf="@+id/imageLocationV"
        app:srcCompat="@mipmap/performtask_beijingkuang4_21" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:shadowColor="#8f0a3658"
        android:shadowDx="0"
        android:shadowDy="1"
        android:shadowRadius="3.0"
        android:text="定位"
        android:textColor="#fff8fafd"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageLocationBt"
        app:layout_constraintEnd_toEndOf="@+id/imageLocationBt"
        app:layout_constraintStart_toStartOf="@+id/imageLocationBt"
        app:layout_constraintTop_toTopOf="@+id/imageLocationBt" />
</androidx.constraintlayout.widget.ConstraintLayout>